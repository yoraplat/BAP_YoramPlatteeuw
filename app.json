{
  "expo": {
    "name": "NoWasteV2",
    "slug": "NoWasteV2",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "nowaste",
    "icon": "./assets/icon.png",
    "notification": {
      "icon": "./assets/icon.png"
    },
    "splash": {
      "image": "./assets/logoV2.1.png",
      "resizeMode": "contain",
      "backgroundColor": "#D2E8D9"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#D2E8D9"
      },
      "package": "com.yoraplat.NoWasteV2"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}